<templateSet group="HTML/XML">
  <template name="fore" value="&lt;?php foreach ($ITERABLE$ as $VAR_VALUE$): ?&gt;&#10;    $END$&#10;&lt;?php endforeach ?&gt;" description="&lt;?php foreach ($array as $value) ?&gt; ... &lt;?php endforeach ?&gt;" toReformat="false" toShortenFQNames="false">
    <variable name="ITERABLE" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="VAR_VALUE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="COMPLETION" value="false" />
      <option name="HTML" value="true" />
      <option name="JSP" value="true" />
    </context>
  </template>
  <template name="php=" value="&lt;?= $END$ ?&gt;" description="PHP short tag" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="HTML" value="true" />
    </context>
  </template>
  <template name="forek" value="&lt;?php foreach ($array as $key =&gt; $value): ?&gt;&#10;    $END$&#10;&lt;?php endforeach ?&gt;" description="&lt;?php foreach ($array as $key =&gt; $value) ?&gt; ... &lt;?php endforeach ?&gt;" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="HTML" value="true" />
    </context>
  </template>
  <template name="var" value="&lt;?= $END$ ?&gt;" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="HTML" value="true" />
      <option name="PHP" value="true" />
    </context>
  </template>
</templateSet>